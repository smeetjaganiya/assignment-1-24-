#include<stdio.h>
int main()
{   float m1,m2,m3,total,avg;
    
    printf("Marks of subject 1: ");
    scanf("%f" , &m1);
    
    printf("Marks of subject 2: ");
    scanf("%f" , &m2);
    
    printf("Marks of subject 3: ");
    scanf("%f" , &m3);
    
    total=m1+m2+m3;
    avg=(m1+m2+m3)/3;
    
    printf("TOTAL MARKS =%.2f\n",total);
    printf("AVERAGE MARKS =%.2f",avg);
    
    return 0;   
}
